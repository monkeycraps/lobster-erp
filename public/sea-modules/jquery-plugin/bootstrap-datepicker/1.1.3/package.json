{
  "family": "jquery-plugin",
  "name": "bootstrap-datepicker",
  "version": "1.1.3",
  "description": "A datepicker for @twitter bootstrap forked from Stefan Petre's (of eyecon.ro), improvements by @eternicode.",
  "homepage": "https://github.com/eternicode/bootstrap-datepicker",
  "author": {
    "name": "Andrew Rowls",
    "url": "https://github.com/eternicode"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eternicode/bootstrap-datepicker.git"
  },
  "bugs": "https://github.com/eternicode/bootstrap-datepicker/issues",
  "licenses": [
    {
      "type": "Apache",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "keywords": [ "datepicker", "bootstrap" ],
  "spm": {
    "alias": {
      "$": "$"
    },
    "output": [
      "datepicker.js",
      "datepicker.css",
      "i18n/zh-cn.js"
    ]
  }
}