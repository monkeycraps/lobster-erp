
<script type='text/template' id='template-formview-add-product-back'>
	<tr>
		<td>
			<input type='hidden' data-name='id' name='send_product_back[<%=uid%>][id]' value='<%=id%>'/>
			<input type='hidden' data-name='category' name='send_product_back[<%=uid%>][category_id]' value='<%=category_id%>'/>
			<input type='hidden' data-name='product' name='send_product_back[<%=uid%>][product_id]' value='<%=product_id%>'/>
			<input type='hidden' data-name='cnt' name='send_product_back[<%=uid%>][cnt]' value='<%=cnt%>'/>
			<input type='hidden' data-name='state' name='send_product_back[<%=uid%>][state]' value='<%=state%>'/>
			<%=category%>	
		</td>
		<td><%=product%></td>
		<td><%=cnt%></td>
		<td><%=state_name%></td>
		<td class='col-lg-4 state_<%= state %> role_<%=app_data.user.role_id %>'>
			<a href='#' class='op delete'><i class='glyphicon glyphicon-remove'></i></a>
			<a href='#' class='op word-btn word-btn-danger ruku'>确认入库</a>
			<a href='#' class='op word-btn word-btn-warning ruku-cancel'>取消入库</a>
		</td>
	</tr>
</script>

